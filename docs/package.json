{
  "name": "docs",
  "private": true,
  "scripts": {
    "start": "relivestyle --single --port 5000 public",
    "update-importmap": "create-styleguide-importmap > public/importmap.js",
    "build": "rm -rf dist && npm run update-import-map && rollup -c node:@dependable/rollup-config",
    "deploy": "npm run build && surge dist dependable-components.surge.sh",
    "test": "mocha"
  },
  "dependencies": {
    "stylewars": "^2.3.0",
    "@dependable/state": "^0.20.0",
    "@dependable/view": "^0.18.0",
    "@dependable/components": "*",
    "@dependable/styleguide": "*",
    "@dependable/icons": "*"
  },
  "devDependencies": {
    "@dependable/rollup-config": "^0.4.0"
  },
  "devDependencies": {
    "relivestyle": "^1.0.0-11"
  }
}
